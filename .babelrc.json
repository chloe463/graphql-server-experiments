{
  "presets": [
    [
      "@babel/env",
      {
        "targets": { "node": "current" },
        "modules": "commonjs"
      }
    ],
    "@babel/preset-typescript"
  ],
  "plugins": [
    "@babel/proposal-class-properties",
    "@babel/proposal-object-rest-spread",
    "@babel/plugin-proposal-nullish-coalescing-operator",
    "@babel/plugin-proposal-optional-chaining"
  ],
  "sourceMaps": "inline",
  "retainLines": true
}
